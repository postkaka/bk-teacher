.payBox {
  margin: 16rpx;  
  background: #FFFFFF;
  box-shadow: 0px 10rpx 40rpx rgba(153, 153, 153, 0.16);
  border-radius: 30rpx;
}
/* 备注 */
.remark{
  margin: 16rpx;
  height: 422rpx;
  background: #FFFFFF;
  box-shadow: 0px 10rpx 40rpx rgba(153, 153, 153, 0.16);
  border-radius: 30rpx;
}
.remark-text {
  position: relative;
  padding: 32rpx;
  width: 100%;
  height: 213rpx;
  top:0rpx;
  overflow-x:hidden;
}
.text-area{
  margin-top: 8rpx;
  font-size:24rpx;
  height: 100rpx;
}
.last-text {
  font-size: 20rpx;
  color: #999999;
  position: relative;
  left: 85%;
  bottom: 20rpx;
}
.last {
  font-size: 20rpx;
  color: #999999;
  position: relative;
  margin-left: 32rpx;
  bottom: 90rpx;
}

.outboundGoods {
  margin: 16rpx;
  height: 348rpx;
  background: #FFFFFF;
  box-shadow: 0px 10rpx 40rpx rgba(153, 153, 153, 0.16);
  border-radius: 30rpx;
}
._outboundGoods {
  padding: 32rpx;
  font-size: 28rpx;
  color: #333333;
  border-bottom: 1rpx solid #DDDDDD;
}
.itemName {
  padding: 32rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #F89470;
  border-bottom: 1rpx solid #DDDDDD;
}
