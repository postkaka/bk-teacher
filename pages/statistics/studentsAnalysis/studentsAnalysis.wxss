/* pages/statistics/studentsAnalysis/studentsAnalysis.wxss */
page {
    overflow: hidden;
  }
  /* 背景 */
  .particulars {
      padding: 50rpx 0rpx 90rpx 0rpx;
      background: #60ADE1;
      font-size: 28rpx;
      height: 90vh;
    }
    .box-text {
        padding: 32rpx;
        font-size: 34rpx;
        color: #FFFFFF;
    }
    .nub {
        margin-top: 8rpx;
    }
    .title {
      text-align: center;
    }
    .boxLine {
        width: 2rpx;
        height: 76rpx;
        background: #FFFFFF;
        border: 1rpx solid #FFFFFF;
        margin-top: 8rpx;
    } 
    .box-text-1 {
        font-size: 24rpx;
        color: #FFFFFF;
        padding-left: 32rpx;
    }
    .last-line {
        height: 26rpx;
        background: #ffffff;
        border: 1rpx solid #ffffff;
        margin: 0 8rpx 0 8rpx;
      }
    .last {
        padding: 32rpx;
        font-size: 24rpx;
        color: #FFFFFF;
    }
    .container {
        width: 100vw;
        height: 40%;
        margin-left: 30rpx;
        margin-top: -80rpx;
      }
      .tab {
        margin: 40rpx 0 30rpx 0;
        height: 100vh;
        background: #FFFFFF;
        border-radius: 50rpx 50rpx 0px 0px;
      }
      .last-title {
        padding: 32rpx 32rpx 16rpx 32rpx;
        font-size: 28rpx;
        color: #333333;
      }
      .one {
        padding: 32rpx 32rpx 16rpx 32rpx;
        border-bottom:1rpx solid #dddddd;
        font-size: 24rpx;
      }
      ._one {
        padding: 16rpx 32rpx 16rpx 32rpx;
        border-bottom:1rpx solid #dddddd;
        font-size: 24rpx;
      }
      .__one {
        padding: 16rpx 32rpx 50rpx 32rpx;
        font-size: 24rpx;
      }
      .center {
          text-align: center;
      }
      ._tab {
          padding: 16rpx 0 0 32rpx;
      }